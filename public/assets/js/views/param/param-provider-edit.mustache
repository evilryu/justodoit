<div class="container col-lg-4 col-lg-offset-4">
	<div class="pull-left param-search">
		<input class="form-control param-search typeahead" type="text" placeholder="Buscar proveedor" can-value='searchFilter'>
	</div>
	<div class="param-form">
		<div class="form-group">
			<input type="text" class="form-control input-hg" placeholder="Nombre" can-value='provider.name'>
		</div>
		<div class="form-group">
			<input type="text" class="form-control input-hg" placeholder="Direccion" can-value='provider.address'>
		</div>
		<div class="form-group">
			<input type="text" class="form-control input-hg" placeholder="Numero Telefono" can-value='provider.phoneNumber'>
		</div>
		<div class="form-group">
			<input type="text" class="form-control input-hg" placeholder="Nombre Contacto" can-value='provider.contact'>
		</div>
		<div class='form-group text-center'>
			<button class="btn btn-info btn-hg" id='update-prov'>Actualizar</button>
			<button class="btn btn-danger btn-hg" id='delete-prov'>Borrar</button>
			<button class="btn btn-warning btn-hg" id='cancel-prov'>Cancel</button>
		</div>
	</div>
</div>